{
  "ConnectionStrings": {
    "OrderService": "",
    "ProductionServiceSender": "",
    "InvoiceServiceSender": "",
    "NotificationServiceSender": ""
  },
  "ExternalService": {
    "PaymentService": ""
  },
  "MessageQueueSettings": {
    "RequestProductionQueueName": "",
    "GenerateInvoiceQueueName": "",
    "SendNotificationQueueName": ""
  },
  "PolicySettings": {
    "Retry": {
      "Enabled": "true",
      "ForStatusCodes": "",
      "Count": "3"
